{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "watch-unocss": "unocss -c unocss.config.ts -o styles/uno.css -w pages/** components/** works/**",
    "build-unocss": "unocss -c unocss.config.ts -o styles/uno.css pages/** components/** works/**",
    "dev": "concurrently \"next dev\" \"pnpm watch-unocss\"",
    "build": "pnpm build-unocss && pnpm next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "npm run check-lint && npm run check-types"
  },
  "dependencies": {
    "@next/font": "13.1.2",
    "@tanstack/react-query": "^4.23.0",
    "contentlayer": "^0.3.0",
    "date-fns": "^2.29.3",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.2",
    "iconsax-react": "^0.0.8",
    "next": "13.1.2",
    "next-contentlayer": "^0.3.0",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^8.0.5",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.1.0"
  },
  "devDependencies": {
    "@iconify-json/logos": "^1.1.23",
    "@iconify-json/ph": "^1.1.4",
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@next/mdx": "^13.1.6",
    "@radix-ui/colors": "^0.1.8",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@unocss/cli": "^0.50.3",
    "@unocss/config": "^0.50.3",
    "@unocss/core": "^0.50.3",
    "@unocss/preset-icons": "^0.48.5",
    "@unocss/preset-uno": "^0.48.5",
    "@unocss/reset": "^0.48.5",
    "@unocss/transformer-variant-group": "^0.48.5",
    "@unocss/webpack": "^0.48.5",
    "concurrently": "^7.6.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.3",
    "typescript": "4.9.4",
    "unocss": "^0.50.3",
    "unocss-preset-theme": "^0.3.0",
    "webpack": "^5.75.0"
  }
}
